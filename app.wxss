/**app.wxss**/
/* 默认背景色 */
page{
  background: #edf2f3;
}
 
/* 隐藏滚动条 */
::-webkit-scrollbar {
    width: 0;
    height: 0;
    color: transparent;
}


.flexBox{
  height:220rpx;
  position: fixed;
  bottom:50rpx;
  left:30rpx;right:30rpx;
  box-sizing: border-box;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
}
.flexBox>view{
  width:100%;
  height:100rpx;
  text-align: center;
  line-height:100rpx;
  color:#fff;
  font-size: 32rpx;
  border-radius:8rpx;
  background-color:#4d80f0;
}
.flexBox>view:first-child{
  background-color:#fff;
  border:1px solid #e1e1e1;
  color:gray;
}

/* 悬浮的新增 */
.fixedImage{
  width:100rpx;
  height:100rpx;
  line-height:100rpx;
  position: fixed;
  right:50rpx;
  bottom:50rpx;
  border-radius:50%;
  background: #4d80f0;
  color:#fff;
  text-align:center;
  font-size: 60rpx;
}

/* 复用的常用语句 */
 .list{
  width:100%;
  margin-bottom:20rpx;
  padding:20rpx;
  box-sizing: border-box;
  border-radius:4rpx;
  background: #fff;
}
.list>view{
  display: flex;
  justify-content: space-between;
}
.list>view .yuan{
  width:16rpx;
  height:16rpx;
  border-radius:50%;
  margin-top:15rpx;
  background:#556ed7;
  display: inline-block;
}
.list>view .yuju{
  width:calc(100% - 40rpx);
  line-height: 50rpx;
  font-size: 30rpx;
  color:#333333;
  display: -webkit-box;
  word-break: break-all;
  margin-bottom:5rpx;
  -webkit-box-orient: vertical;
  -webkit-line-clamp:4;
  overflow: hidden;
  text-overflow:ellipsis;
}
.list .time{
  float:right;
  color:#7e7e7e;
  font-size:26rpx;
}
.list:after{
  content:"";
  clear: both;
  display: inline-block;
}



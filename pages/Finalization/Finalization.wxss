/**index.wxss**/
.container{
  width:100%;
  height:100%;
  padding:20rpx;
  box-sizing: border-box;
}
/* 搜索栏目 */
.container .inputBox{
  width:100%;
  height:80rpx;
  background: #4d80f0;
  border-radius:10rpx;
  padding-left:10rpx;
  box-sizing:border-box;
  color:#fff;
  line-height: 80rpx;
  text-align: center;
  position: relative;
}
.container .back{
  width:100%;
  background: #edf2f3;
  padding:20rpx 20rpx 0;
  box-sizing:border-box;
  position: fixed;
  left:0;
  top:0;
  right:0;z-index:999
}
.container image.iconfong{
  width:30rpx;
  display: inline-block;
  vertical-align: middle;
}
.scrollBox{
  margin-top:125rpx;
}

.textCon{
  width:100%;
  background: #fff;
  margin-bottom: 20rpx;
  padding:5rpx 10rpx;
  box-sizing:border-box;
}
.textCon .title{
  padding: 20rpx 0;
  width: 100%;
  font-size: 30rpx;
  display: flex;
  align-items: center;
}
.textCon .title .circular{
  width: 20rpx;
  height: 20rpx;
  display: inline-block;
  vertical-align: middle;
  background: #4d81f1;
  border-radius: 50%;
  margin-right: 10rpx;
}
.textCon .title .titleText{
  flex: 1;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.textCon .text{
  padding: 15rpx 15rpx;
  line-height: 44rpx;
  font-size: 28rpx;
  color: #646464;
  display: block;
  position: relative;
  overflow: hidden;
  border-top: 1rpx solid #e1e1e1;
  text-overflow: -o-ellipsis-lastline;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 5;
  -webkit-box-orient: vertical;
}


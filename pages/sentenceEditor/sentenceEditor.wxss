/* pages/manEntry/manEntry.wxss */
page{
  background:#fff
}
.entryBox{
  width:100%;
  padding:20rpx 30rpx;
  box-sizing: border-box;
}
.entryBox .entryList{
  display: flex;
  width:100%;
  border-bottom:1px solid #f0f0f0;
  padding:20rpx 0;
  margin-bottom:10rpx;
  font-size:30rpx;
  color:#2b2828;
}
.entryBox .entryList>text:first-child{
  display:inline-block;
  min-width:120rpx;
}
.entryBox .entryList input{
  flex:1;
  padding-left:40rpx;
  box-sizing: border-box;
   font-size:28rpx;
}
.conImport{
  width:100%;
  font-size:30rpx;
  color:#2b2828;
}
.conImport>view:not(.textareaBox){
  padding:20rpx 0;
}
.conImport .textareaBox{
  position: relative;
}
.conImport .textareaBox .operation{
  width:100%;
  display: flex;
  padding-top:20rpx;
  justify-content: flex-end;
}
.conImport .textareaBox .operation>text{
  width:40rpx;
  height:40rpx;
  border-radius: 50%;
  background-color:#6eabfe;
  text-align:center;
  line-height: 40rpx;
  color:#fff;
}
.conImport .textareaBox .operation .reduce{
  margin-left:20rpx;
  background:#c4c4c4;
}
.selectCompany{
  margin-left:5px;
  display: inline-block;
  width: 100%;
}
.reduce{
  width:40rpx;
  height:40rpx;
  border-radius: 50%;
  text-align:center;
  display: inline-block;
  margin-left:20rpx;
  background:#c4c4c4;
  line-height: 40rpx;
  color:#fff;
}
.conImport .textareaBox>textarea{
  width:100%;
  height:280rpx;
  border:1px solid #f0f0f0;
  padding:20rpx;
  box-sizing: border-box;
  font-size:27rpx;
}
.conImport editor{
  min-height:555rpx;
  margin-bottom:20rpx;
}
.placeView{
  color:#e1e1e1;
  font-size:28rpx;
  line-height: 34rpx;
  padding-left:40rpx;
  flex:1;
  max-width: 75%;
  word-break: break-all;
  word-warp: break-word;
}
.placeView view{
  color:#000;
  padding:6rpx 0;
}
.entryList picker .selectJb{
  color:gray;
  font-size: 28rpx;
}
.entryList picker .selectJb.Dark{
  color:#e1e1e1;
}
.entryList picker{
  width:calc(100% - 150rpx);
  position: relative;
  padding-left:40rpx;
  box-sizing:border-box;
}
.entryList picker .xiala{
  width:25rpx;
  height:15rpx;
  position: absolute;
  right:0;
  top:0;
  bottom:0;
  margin:auto;
}
textarea{
  margin-bottom:10rpx;
}
.name{
  position:relative;
}
.name:after{
  content:"*";
  position:absolute;
  color:red;
  right:-20rpx;
  top:0;
  font-size:32rpx;
}
.flexBox{
  width:95%
}
.entryBox .entryList.removePading text{margin-right:30rpx}
.mytree{margin-left: -33rpx;width:100%}


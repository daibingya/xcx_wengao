/* pages/fina-message/fina-message.wxss */
page{
  background: #fff;
}
.manDetailsBox{
  width:100%;
  padding:10rpx 30rpx;
  box-sizing:border-box;
}
.manDetailsBox .title{
  font-size:38rpx;
  color:#333333;
  text-align: center;
  padding:30rpx 0;
}
.information{
  width:100%;
  padding:15rpx 30rpx;
  box-sizing: border-box;
  background-color:#fbfbfb;
  border-radius:4rpx;
  display: flex;
  font-size:20rpx;
  color:#999999;
  line-height: 42rpx;
  justify-content: space-between;
  margin-bottom:30rpx;
}
.information>view{width:50%}
.Enclosure{
  width:100%;
  height:auto;
  padding:10rpx 0;
  box-sizing: border-box;
  font-size:28rpx;
  color:#333333;
  display: flex;
  justify-content: space-between;
  margin:20rpx 0 0;
}
.Enclosure image{
  width:30rpx;
  height:30rpx;
}
.shera{
 width:100%;
 text-align: center;
 font-size:32rpx;
 color:#fff;
 background-color:#4d80f0;
 border-radius:4rpx;
 margin:30rpx 0;
}